{
    "manifest_version": 3,
    "name": "Salesforce Audit Trail Checker",
    "version": "1.0",
    "description": "A Chrome extension to check audit trails in Salesforce.",
    "permissions": [
      "activeTab",
      "storage"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  